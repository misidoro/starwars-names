{
  "name": "real-starwars-names",
  "version": "0.0.0-semantically-released",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/misidoro/starwars-names.git"
  },
  "keywords": [
    "random",
    "starwars",
    "star",
    "wars"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Miguel Isidoro <mac.isidoro@gmail.com> (https://misidoro.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misidoro/starwars-names/issues"
  },
  "homepage": "https://github.com/misidoro/starwars-names#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js --mode development",
    "build:umd.min": "webpack --output-filename index.umd.min.js --mode production",
    "commit": "git-cz",
    "cover": "nyc npm t",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "watch:test": "npm t -- -w",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/index.test.js --compilers js:babel-register"
  },
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codecov.io": "^0.1.6",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "^2.0.4",
    "mocha": "^5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "^11.8.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.5.0",
    "travis-deploy-once": "^5.0.0",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
