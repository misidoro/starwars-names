{
  "name": "real-starwars-names",
  "version": "0.0.0-semantically-released",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -R spec src/index.test.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov | codecov"
  },
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov.io": "^0.1.6",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "^2.0.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "semantic-release": "^15.5.0",
    "travis-deploy-once": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/misidoro/starwars-names.git"
  },
  "keywords": [
    "random",
    "starwars",
    "star",
    "wars"
  ],
  "author": "Miguel Isidoro <mac.isidoro@gmail.com> (https://misidoro.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/misidoro/starwars-names/issues"
  },
  "homepage": "https://github.com/misidoro/starwars-names#readme",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
